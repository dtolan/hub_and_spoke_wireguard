{
  "name": "@dtolan/hub-and-spoke-wireguard",
  "version": "0.1.0",
  "description": "React/TypeScript dashboard for managing WireGuard hub-and-spoke VPN configurations",
  "type": "module",
  "main": "./dist/hub-and-spoke-wireguard.umd.cjs",
  "module": "./dist/hub-and-spoke-wireguard.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/hub-and-spoke-wireguard.js",
      "require": "./dist/hub-and-spoke-wireguard.umd.cjs",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "dev:backend": "tsx watch src/backend/server.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:backend\"",
    "build": "tsc && vite build",
    "build:lib": "tsc && vite build --mode library",
    "build:backend": "tsc --project tsconfig.backend.json",
    "backend:start": "node dist/backend/server.js",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "db:migrate": "tsx src/backend/config/migrate.ts",
    "generate:pdf": "node scripts/generate-pdf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dtolan/hub_and_spoke_wireguard.git"
  },
  "keywords": [
    "wireguard",
    "vpn",
    "hub-and-spoke",
    "dashboard",
    "react",
    "typescript",
    "configuration-management"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dtolan/hub_and_spoke_wireguard/issues"
  },
  "homepage": "https://github.com/dtolan/hub_and_spoke_wireguard#readme",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/markdown-it": "^13.0.7",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "markdown-it": "^14.0.0",
    "markdown-it-anchor": "^8.6.7",
    "puppeteer": "^21.6.1",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.7.0"
  },
  "dependencies": {
    "better-sqlite3": "^9.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "ipaddr.js": "^2.1.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  }
}
